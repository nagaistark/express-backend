{
   "experimentalTernaries": false,
   "printWidth": 80,
   "tabWidth": 3,
   "useTabs": false,
   "semi": true,
   "singleQuote": true,
   "quoteProps": "as-needed",
   "jsxSingleQuote": true,
   "trailingComma": "es5",
   "bracketSpacing": true,
   "objectWrap": "preserve",
   "bracketSameLine": false,
   "arrowParens": "avoid",
   "proseWrap": "preserve",
   "htmlWhitespaceSensitivity": "css",
   "endOfLine": "lf",
   "embeddedLanguageFormatting": "auto",
   "singleAttributePerLine": false
}
